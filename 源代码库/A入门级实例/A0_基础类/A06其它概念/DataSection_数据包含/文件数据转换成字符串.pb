FileName$ = "PureBasic.ico"
FileSize = FileSize(FileName$)
*MemData = AllocateMemory(FileSize+100)

FileID = ReadFile(#PB_Any, FileName$)
If FileID
   ReadData(FileID, *MemData, FileSize) 
   CloseFile(FileID)
   Debug "DataSection"
   Debug "_ICO_PureBasic:"
   For k = 0 To FileSize-1 Step 4
      Text$ + ", $"+RSet(Hex(PeekL(*MemData+k), #PB_Long), 8, "0")
      Index+1
      If Index >= 10
         Debug "   Data.l "+ Mid(Text$, 2)
         Text$ = #Null$ 
         Index = 0 
      EndIf 
   Next 
   Debug "   Data.l "+ Mid(Text$, 2)
   Debug "EndDataSection"
EndIf 

